{
  "name": "vue-calendar",
  "version": "1.0.0",
  "description": "VueJS Calendar Plugin",
  "main": "index.js",
  "repository": "git@github.com:anvyst/vue-calendar.git",
  "author": "andrey@7winds.tech",
  "license": "MIT",
  "scripts": {
    "lint:js": "eslint --ext js --ext vue src",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.dev.config.js --progress --hide-modules",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "watch": "webpack-dashboard -- webpack-dev-server --config ./build/webpack.dev.config.js --open"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
